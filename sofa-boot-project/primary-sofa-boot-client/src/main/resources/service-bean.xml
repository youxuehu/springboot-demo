<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:sofa="http://sofastack.io/schema/sofaboot"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd
                           http://sofastack.io/schema/sofaboot
                           http://sofastack.io/schema/sofaboot.xsd">


    <sofa:reference jvm-first="false" id="helloSyncGenericServiceReference" interface="com.alipay.sofa.rpc.api.GenericService">
        <sofa:binding.h2c>
            <sofa:global-attrs generic-interface="org.example.service.HelloSyncService"/>
        </sofa:binding.h2c>
    </sofa:reference>


    <sofa:reference jvm-first="false" id="testGenericServiceReference" interface="com.alipay.sofa.rpc.api.GenericService">
        <sofa:binding.h2c>
            <sofa:global-attrs generic-interface="org.example.service.TestService"/>
        </sofa:binding.h2c>
    </sofa:reference>

</beans>