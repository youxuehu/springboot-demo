<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd
                           http://www.springframework.org/schema/aop
                           http://www.springframework.org/schema/aop/spring-aop.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="demoTaskService" class="org.example.db.service.demotask.impl.DemoTaskServiceImpl">
        <property name="demoTaskMapper" ref="demoTaskMapper"/>
    </bean>
    <bean id="bizLockService" class="org.example.db.service.bizlock.impl.BizLockServiceImpl">
        <property name="bizLockMapper" ref="bizLockMapper"/>
    </bean>
    <bean id="distributeLock" class="org.example.lock.DistributeLock">
        <property name="bizLockService" ref="bizLockService"/>
    </bean>
    <bean id="testLock" class="org.example.lock.TestLock" init-method="initLock">
        <property name="distributeLock" ref="distributeLock"/>
    </bean>

</beans>